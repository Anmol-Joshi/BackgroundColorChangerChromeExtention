{
  "manifest_version": 3,
  "name": "Background Color Changer",
  "version": "1.0",
  "description": "A simple extension to change the background color of a page",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon.png",
      "48": "icon.png",
      "128": "icon.png"
    }
  },
  "permissions": ["activeTab", "scripting"]
}
